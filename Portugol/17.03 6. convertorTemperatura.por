programa {
  funcao inicio() {
    //Faça um programa onde o usuário deve digitar a temperatura em graus Celsius e mostrar na tela essa temperatura convertida em Fahrenheit.
   real celcius , fahrenheit
   escreva ("Digite uma temperatura em graus Celcius : ")
   leia (celcius)
   fahrenheit = (celcius * 1.8 + 32)
   leia ("A temperatura convertida em graus Fahrenheit e: " , fahrenheit)
  }
}
