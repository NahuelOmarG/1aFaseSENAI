programa {
  funcao inicio() {
    
    inteiro numeroUm , numeroDois
    real media 

    escreva ("Digite o valor da primer numero: ")
    leia (numeroUm)
    escreva ("Digite o valor do segundo numero: ")
    leia (numeroDois)

    media = ( numeroUm + numeroDois ) / 2

    escreva ("A media dos numeros e: " , media)
  }
}
