programa {
  funcao inicio() {
    
    inteiro numeroUm , numeroDois
    inteiro calculoUm , calculoDois , somaQuadrados

    escreva ("Digite o primeiro numero: ")
    leia (numeroUm)
    escreva ("Digite o segundo numero: ")
    leia (numeroDois)

    calculoUm = numeroUm * numeroUm
    calculoDois = numeroDois * numeroDois
    somaQuadrados = calculoUm + calculoDois

    escreva("Soma dos quadrados: " , somaQuadrados)

  }
}
