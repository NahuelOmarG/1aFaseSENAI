programa {
  funcao inicio() {
    
    //Declaraçao das variáveis

      inteiro numeroUm, numeroDois

      //Entrada de dados
      escreva("Digite o primeiro numero: \n" ) 
      // \n = Quebra de linha
      leia (numeroUm)
       escreva("Digite o segundo numero: \n" ) 
      // \n = Quebra de linha
      leia (numeroDois)

      //Saida de dados
      escreva(numeroUm + numeroDois)


    
  }
}
